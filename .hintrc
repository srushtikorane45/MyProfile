{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ]
  }
}